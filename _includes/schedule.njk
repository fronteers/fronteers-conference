            <td>
                08:30 – 09:15
            </td>
            <td>
                <p>Doors open and registration</p>
            </td>
        </tr>
      </table>
    </div>
  </section>

  <section class="block bottom">
      <div class="rainbow">
        <h3>Day two</h3>
        <table class='schedule'>
          <tr>
              <td>
                  08:30 – 09:15
              </td>
              <td>
                  <p>Doors open and registration</p>
              </td>
          </tr>
        </table>
      </div>
    </section>
{% endblock %}
